{
  "name": "anidb",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Rin Minase",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/RinMinase/anidb-mobile"
  },
  "scripts": {
    "clean": "cd android && ./gradlew clean && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "setup": "npm run setup:create && npm run setup:update",
    "setup:create": "react-native init anidb --version 0.62.1 && rm -rf ios/ android/",
    "setup:update": "mv anidb/ios ios && mv anidb/android android && rm -rf anidb"
  },
  "dependencies": {
    "react": "16.11.0",
    "react-native": "0.62.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "metro-react-native-babel-preset": "^0.58.0",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.13.1"
  },
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ]
  }
}
